@model IEnumerable<int>

@{
    ViewData["Title"] = "Home Page";
}


<form id="reverseForm" asp-action="Loop" method="post">
    <button type="submit">Run</button>
    
   
    <textarea rows="10" cols="20" readonly>
        @foreach (var number in Model)
        {
            @number
        }
    </textarea>
   
</form>





